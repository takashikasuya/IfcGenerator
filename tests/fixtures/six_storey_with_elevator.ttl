@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix bot:  <https://w3id.org/bot#> .
@prefix sbco: <https://www.sbco.or.jp/ont/> .
@prefix topo: <https://topo2ifc.example.org/ont#> .
@prefix ex:   <https://example.com/> .

ex:bldg a bot:Building ; bot:hasStorey ex:l1, ex:l2, ex:l3, ex:l4, ex:l5, ex:l6 .

ex:l1 a bot:Storey ; rdfs:label "L1" ; topo:elevation "0.0"^^xsd:float ; bot:hasSpace ex:office_l1, ex:elevator_core_l1 .
ex:l2 a bot:Storey ; rdfs:label "L2" ; topo:elevation "3.0"^^xsd:float ; bot:hasSpace ex:office_l2, ex:elevator_core_l2 .
ex:l3 a bot:Storey ; rdfs:label "L3" ; topo:elevation "6.0"^^xsd:float ; bot:hasSpace ex:office_l3, ex:elevator_core_l3 .
ex:l4 a bot:Storey ; rdfs:label "L4" ; topo:elevation "9.0"^^xsd:float ; bot:hasSpace ex:office_l4, ex:elevator_core_l4 .
ex:l5 a bot:Storey ; rdfs:label "L5" ; topo:elevation "12.0"^^xsd:float ; bot:hasSpace ex:office_l5, ex:elevator_core_l5 .
ex:l6 a bot:Storey ; rdfs:label "L6" ; topo:elevation "15.0"^^xsd:float ; bot:hasSpace ex:office_l6, ex:elevator_core_l6 .

ex:office_l1 a bot:Space ; rdfs:label "Office L1" ; topo:areaTarget "16.0"^^xsd:float ; bot:isSpaceOf ex:l1 .
ex:office_l2 a bot:Space ; rdfs:label "Office L2" ; topo:areaTarget "16.0"^^xsd:float ; bot:isSpaceOf ex:l2 .
ex:office_l3 a bot:Space ; rdfs:label "Office L3" ; topo:areaTarget "16.0"^^xsd:float ; bot:isSpaceOf ex:l3 .
ex:office_l4 a bot:Space ; rdfs:label "Office L4" ; topo:areaTarget "16.0"^^xsd:float ; bot:isSpaceOf ex:l4 .
ex:office_l5 a bot:Space ; rdfs:label "Office L5" ; topo:areaTarget "16.0"^^xsd:float ; bot:isSpaceOf ex:l5 .
ex:office_l6 a bot:Space ; rdfs:label "Office L6" ; topo:areaTarget "16.0"^^xsd:float ; bot:isSpaceOf ex:l6 .

ex:elevator_core_l1 a bot:Space ; rdfs:label "Main Elevator" ; topo:category "core" ; topo:areaTarget "6.0"^^xsd:float ; bot:isSpaceOf ex:l1 .
ex:elevator_core_l2 a bot:Space ; rdfs:label "Main Elevator" ; topo:category "core" ; topo:areaTarget "6.0"^^xsd:float ; bot:isSpaceOf ex:l2 .
ex:elevator_core_l3 a bot:Space ; rdfs:label "Main Elevator" ; topo:category "core" ; topo:areaTarget "6.0"^^xsd:float ; bot:isSpaceOf ex:l3 .
ex:elevator_core_l4 a bot:Space ; rdfs:label "Main Elevator" ; topo:category "core" ; topo:areaTarget "6.0"^^xsd:float ; bot:isSpaceOf ex:l4 .
ex:elevator_core_l5 a bot:Space ; rdfs:label "Main Elevator" ; topo:category "core" ; topo:areaTarget "6.0"^^xsd:float ; bot:isSpaceOf ex:l5 .
ex:elevator_core_l6 a bot:Space ; rdfs:label "Main Elevator" ; topo:category "core" ; topo:areaTarget "6.0"^^xsd:float ; bot:isSpaceOf ex:l6 .

ex:elev1 a sbco:Elevator ; sbco:name "Main Elevator" ; sbco:locatedIn ex:elevator_core_l1 .
ex:elev2 a sbco:Elevator ; sbco:name "Main Elevator" ; sbco:locatedIn ex:elevator_core_l2 .
ex:elev3 a sbco:Elevator ; sbco:name "Main Elevator" ; sbco:locatedIn ex:elevator_core_l3 .
ex:elev4 a sbco:Elevator ; sbco:name "Main Elevator" ; sbco:locatedIn ex:elevator_core_l4 .
ex:elev5 a sbco:Elevator ; sbco:name "Main Elevator" ; sbco:locatedIn ex:elevator_core_l5 .
ex:elev6 a sbco:Elevator ; sbco:name "Main Elevator" ; sbco:locatedIn ex:elevator_core_l6 .
