@prefix sbco: <https://www.sbco.or.jp/ont/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

<urn:sbco:level:1> a sbco:Level ;
    sbco:name "Level 1" ;
    sbco:elevation "0.0"^^xsd:double ;
    sbco:hasPart <urn:sbco:space:office>, <urn:sbco:space:meeting> .

<urn:sbco:space:office> a sbco:Space ;
    sbco:name "Office" ;
    sbco:isPartOf <urn:sbco:level:1> .

<urn:sbco:space:meeting> a sbco:Space ;
    sbco:name "Meeting" ;
    sbco:isPartOf <urn:sbco:level:1> .

<urn:sbco:eq:ahu-01> a sbco:Equipment ;
    sbco:name "AHU-01" ;
    sbco:locatedIn <urn:sbco:space:office> ;
    sbco:deviceType "AHU" ;
    sbco:maintenanceInterval "P6M" ;
    sbco:hasPoint <urn:sbco:point:temp-01> .

<urn:sbco:eq:vav-01> a sbco:EquipmentExt ;
    sbco:name "VAV-01" ;
    sbco:locatedIn <urn:sbco:space:meeting> ;
    sbco:deviceType "VAV" ;
    sbco:maintenanceInterval "P12M" ;
    sbco:hasPoint <urn:sbco:point:cmd-01> .

<urn:sbco:point:temp-01> a sbco:Point ;
    sbco:name "Temp-01" ;
    sbco:pointType "temperature" ;
    sbco:unit "degC" ;
    sbco:hasQuantity "Temperature" .

<urn:sbco:point:cmd-01> a sbco:PointExt ;
    sbco:name "Cmd-01" ;
    sbco:pointType "command" ;
    sbco:unit "percent" ;
    sbco:hasQuantity "ValvePosition" .
